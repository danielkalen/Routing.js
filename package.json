{
  "name": "routing.js",
  "version": "1.1.0-b",
  "description": "Simple, lightweight, promise-based client-side routing supporting IE8+",
  "main": "dist/index.js",
  "browser": {
    "./debug": "dist/index.debug.js",
    "./src": "src/index.coffee"
  },
  "scripts": {
    "postversion": "npm run build",
    "test": "npm run test:electron -s || true",
    "test:local": "open test/testrunner.html",
    "test:minified": "minified=1 npm run test:electron -s || true",
    "test:karma": "karma start .config/karma.conf.coffee",
    "test:electron": "karma start --single-run --browsers Electron .config/karma.conf.coffee",
    "test:chrome": "karma start --single-run --browsers Chrome .config/karma.conf.coffee",
    "test:safari": "karma start --single-run --browsers Safari .config/karma.conf.coffee",
    "test:firefox": "karma start --single-run --browsers Firefox .config/karma.conf.coffee",
    "test:sauce": "coffee .config/sauce-launch.coffee && npm run test-serve",
    "test-serve": "http-server -p 9201 --cors ./",
    "sauce-connect": "sc -u routingjs -k 9321bba9-2293-4e66-8fb5-06e255fbaa51",
    "build": "npm run compile && npm run minify",
    "compile": "cake build",
    "compile:debug": "cake -d build",
    "minify": "npm run minify:super && npm run minify:manual && npm run minify:simple",
    "minify:simple": "uglifyjs dist/index.debug.js -m -c keep_fargs,unused=false -o dist/index.js",
    "minify:super": "closure-service dist/index.debug.js > dist/index.js",
    "minify:manual": "coffee .config/manual-minify.coffee",
    "watch": "cake watch",
    "watch:debug": "cake -d watch",
    "coverage": "npm run coverage:run; npm run coverage:badge",
    "coverage:run": "coverage=true npm run test:electron",
    "coverage:badge": "badge-gen -d ./.config/badges/coverage",
    "coverage:show": "open coverage/lcov-report/index.html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielkalen/routing.js.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielkalen/routing.js/issues"
  },
  "homepage": "https://github.com/danielkalen/routing.js#readme",
  "devDependencies": {
    "badge-gen": "^1.0.1",
    "bluebird": "^3.5.0",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "closure-compiler-service": "^0.6.1",
    "coffee-script": "^1.12.4",
    "electron": "^1.6.2",
    "foreach-cli": "^1.7.1",
    "jquery": "^3.2.1",
    "json": "^9.0.6",
    "karma": "^1.5.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-electron-launcher": "^0.2.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-opera-launcher": "^1.0.0",
    "karma-safari-launcher": "^1.0.0",
    "listr": "^0.12.0",
    "mocha": "^3.2.0",
    "simplyimport": "^4.0.0-s25",
    "simplywatch": "^3.0.0-l2",
    "smart-extend": "^1.7.3",
    "uglify-js": "^3.0.19"
  },
  "dependencies": {}
}
